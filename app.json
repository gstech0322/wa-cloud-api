{
  "expo": {
    "name": "WhatsApp Cloud",
    "slug": "sailbot-app",
    "owner": "ttfh",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "assetBundlePatterns": ["**/*"],
    "notification": {
      "icon": "./app/assets/notification.png",
      "color": "#008069",
      "androidCollapsedTitle": "WhatsApp Cloud"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "biz.sailbot.app"
    },
    "android": {
      "package": "biz.sailbot.app",
      "googleServicesFile": "./google-services.json",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "notification": {
        "icon": "./app/assets/notification.png",
        "color": "#008069"
      }
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    }
  }
}
